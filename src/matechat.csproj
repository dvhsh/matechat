<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>disable</Nullable>
		<RootNamespace>matechat</RootNamespace>
		<LangVersion>latest</LangVersion>
		<IsPackable>false</IsPackable>

		<!-- Define version information -->
		<AssemblyVersion>1.2.1</AssemblyVersion>
		<FileVersion>1.2.1</FileVersion>
		<InformationalVersion>1.2.1</InformationalVersion>
		<AssemblyName>matechat</AssemblyName>
		<OutputType>Library</OutputType>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<!-- Define possible game paths -->
		<GamePathProgramFiles>C:\Program Files (x86)\Steam\steamapps\common\Desktop Mate</GamePathProgramFiles>
		<GamePathSteamLibrary>C:\SteamLibrary\steamapps\common\Desktop Mate</GamePathSteamLibrary>
	</PropertyGroup>

	<PropertyGroup>
		<StartAction>Program</StartAction>
		<StartProgram>C:\Windows\System32\cmd.exe</StartProgram>
		<StartArguments>/C start steam://run/3301060</StartArguments>
	</PropertyGroup>

	<Target Name="InjectVersion" BeforeTargets="BeforeBuild">
		<PropertyGroup>
			<ConstantsFile>$(ProjectDir)Constants.cs</ConstantsFile>
			<ConstantsBackupFile>$(ProjectDir)Constants.cs.bak</ConstantsBackupFile>
		</PropertyGroup>

		<!-- Restore the backup to ensure PLACEHOLDER_VERSION exists -->
		<Exec Command="copy /Y &quot;$(ConstantsBackupFile)&quot; &quot;$(ConstantsFile)&quot;" />

		<!-- Replace PLACEHOLDER_VERSION with the actual version -->
		<Exec Command="powershell -Command &quot;(Get-Content '$(ConstantsFile)') -replace 'PLACEHOLDER_VERSION', '$(FileVersion)' | Set-Content '$(ConstantsFile)'&quot;" />
	</Target>
	
	<Target Name="PostBuild" AfterTargets="PostBuildEvent">
		<!-- Define mod folders -->
		<PropertyGroup>
			<ModPathProgramFiles>$(GamePathProgramFiles)\Mods</ModPathProgramFiles>
			<ModPathSteamLibrary>$(GamePathSteamLibrary)\Mods</ModPathSteamLibrary>
		</PropertyGroup>

		<!-- Create Mods directory if it doesn't exist -->
		<MakeDir Directories="$(ModPathProgramFiles)" Condition="Exists('$(GamePathProgramFiles)') And !Exists('$(ModPathProgramFiles)')" />
		<MakeDir Directories="$(ModPathSteamLibrary)" Condition="Exists('$(GamePathSteamLibrary)') And !Exists('$(ModPathSteamLibrary)')" />

		<!-- Copy to appropriate Mods folder -->
		<Message Text="Copying to Program Files path..." Condition="Exists('$(GamePathProgramFiles)')" Importance="high" />
		<Copy SourceFiles="$(TargetPath)" DestinationFolder="$(ModPathProgramFiles)" Condition="Exists('$(GamePathProgramFiles)')" />

		<Message Text="Copying to SteamLibrary path..." Condition="Exists('$(GamePathSteamLibrary)')" Importance="high" />
		<Copy SourceFiles="$(TargetPath)" DestinationFolder="$(ModPathSteamLibrary)" Condition="Exists('$(GamePathSteamLibrary)')" />
	</Target>

