	<Target Name="PostBuild" AfterTargets="Build">
		<!-- Rename matechat.dll to matechat.$(SanitizedVersion).dll -->
		<Exec Command="if exist $(OutputPath)matechat.$(FileVersion).dll del $(OutputPath)matechat.$(FileVersion).dll" />
		<Exec Command="move /Y $(OutputPath)matechat.dll $(OutputPath)matechat.$(FileVersion).dll" />
	</Target>

	<Target Name="PostBuildCopy" AfterTargets="PostBuild">
		<PropertyGroup>
			<TargetModsFolder Condition="Exists('$(GamePathProgramFiles)')">$(GamePathProgramFiles)\Mods</TargetModsFolder>
			<TargetModsFolder Condition="!Exists('$(GamePathProgramFiles)') And Exists('$(GamePathSteamLibrary)')">$(GamePathSteamLibrary)\Mods</TargetModsFolder>
		</PropertyGroup>

		<!-- Log paths for debugging -->
		<Message Importance="High" Text="TargetModsFolder: $(TargetModsFolder)" />
		<Message Importance="High" Text="OutputPath: $(OutputPath)" />

		<!-- Ensure Mods folder exists -->
		<Exec Condition="Exists('$(TargetModsFolder)') == false" Command="mkdir &quot;$(TargetModsFolder)&quot;" />

		<!-- Delete any existing matechat files in the Mods folder, if they exist -->
		<Exec Condition="Exists('$(TargetModsFolder)') And Exists('$(TargetModsFolder)\matechat*')" Command="del /Q &quot;$(TargetModsFolder)\matechat*&quot;" />

		<!-- Copy the renamed file to the appropriate Mods folder -->
		<Exec Condition="Exists('$(TargetModsFolder)')" Command="xcopy &quot;$(OutputPath)matechat.$($(FileVersion)).dll&quot; &quot;$(TargetModsFolder)&quot; /Y" />
	</Target>
